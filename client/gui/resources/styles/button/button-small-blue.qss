QPushButton {
    font-size: 12pt;
    color: rgb(255, 255, 255);
    background-color: rgb(12, 140, 233);
    border-radius: 10px;
    padding: 5px 10px 5px 10px;
}

QPushButton:hover {
    background-color: rgb(11, 125, 209);
}

QPushButton:!enabled {
    color: rgb(200, 200, 200);
    background-color: rgb(245, 245, 245);
    border: 1px solid rgb(200, 200, 200);
}
